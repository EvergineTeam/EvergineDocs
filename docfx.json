{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Engine/Common/WaveEngine.Common/WaveEngine.Common.csproj",
            "src/Engine/Rendering/WaveEngine.OpenGL/WaveEngine.OpenGL.csproj",
            "src/Engine/WindowSystems/WaveEngine.SDL/WaveEngine.SDL.csproj",
            "src/Engine/Rendering/WaveEngine.DirectX11/WaveEngine.DirectX11.csproj",
            "src/Engine/WindowSystems/WaveEngine.Forms/WaveEngine.Forms.csproj",
            "src/Engine/WindowSystems/WaveEngine.WPF/WaveEngine.WPF.csproj",
            "src/Engine/Common/WaveEngine.Mathematics/WaveEngine.Mathematics.csproj",
            "src/Engine/WindowSystems/WaveEngine.GTK/WaveEngine.GTK.csproj",
            "src/Engine/Common/WaveEngine.Framework/WaveEngine.Framework.csproj",
            "src/Engine/Physics/WaveEngine.Bullet/WaveEngine.Bullet.csproj",
            "src/Engine/Platform/WaveEngine.Platform.csproj",
            "src/Engine/Rendering/WaveEngine.DirectX12/WaveEngine.DirectX12.csproj",
            "src/Engine/Rendering/WaveEngine.Vulkan/WaveEngine.Vulkan.csproj",
            "src/Engine/Rendering/WaveEngine.Metal/WaveEngine.Metal.csproj",
            "src/Engine/Common/WaveEngine.Assets/WaveEngine.Assets.csproj",
            "src/Engine/Audio/WaveEngine.XAudio2/WaveEngine.XAudio2.csproj",
            "src/Engine/Audio/WaveEngine.OpenAL/WaveEngine.OpenAL.csproj",
            "src/Tools/WaveEngine.HLSLEverywhere/WaveEngine.HLSLEverywhere.csproj",
            "src/Engine/Common/WaveEngine.Components/WaveEngine.Components.csproj",
            "src/Tools/WaveEngine.Converter/WaveEngine.Converter.csproj",
            "src/Extensions/WaveEngine.OpenVR/WaveEngine.OpenVR.csproj",
            "src/Engine/Rendering/WaveEngine.WebGL/WaveEngine.WebGL.csproj",
            "src/Extensions/WaveEngine.NoesisGUI/WaveEngine.NoesisGUI.csproj",
            "src/Extensions/WaveEngine.Vuforia/WaveEngine.Vuforia/WaveEngine.Vuforia.csproj",
            "src/Extensions/WaveEngine.Vuforia/VuforiaSharp/VuforiaSharp.csproj",
            "src/Extensions/WaveEngine.Networking/WaveEngine.Networking.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api3preview",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "version": "v2.5"
      },
      {
        "files": [
          "api3preview/**.yml",
          "api3preview/index.md"
        ],
        "version": "v3.0"
      },
      {
        "files": [
          "recipes/**.md",
          "recipes/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "recipes/**/images/**",
		  "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**",
          "api3preview/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../DocOnline",
    "globalMetadata": {
      "_appTitle": "Wave Engine API Doc",
      "_enableSearch": "true",
      "_disableContribution": "true",
      "_gitContribute": {
        "repo": "https://github.com/WaveEngine/Documentation.git",
        "branch": "develop"
      }
    },
    "fileMetadataFiles": [],
    "template": [     "default","template"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}